LIBS = libdomx.a $(XERCESC_LIBS) $(LOGX_LIBS) 

CccProgramTarget(xmlfilescan,xmlfilescan.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))
CccProgramTarget(xmlcatalog,xmlcatalog.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

